#include<bits/stdc++.h>
#define ll long long
using namespace std;

int n, k;
vector<int> a(500001);
deque<int> dq;


int main()
{
	ios::sync_with_stdio(0);
	cin.tie(0);
	
	cin >> n >> k;
	for(int i = 1; i <= n; i++) cin >> a[i];
	for(int i = 1; i < k; i++)
	{
		while(dq.size() > 0 && a[i] < a[dq.back()]) dq.pop_back();
		dq.push_back(i);
	}
	for(int i = k; i <= n; i++)
	{
		while(dq.size() > 0 && a[i] < a[dq.back()]) dq.pop_back();
		if(dq.front() == i-k) dq.pop_front();
		dq.push_back(i);
		cout << a[dq.front()] << '\n';
	}
}
