#include<bits/stdc++.h>
#define ll long long
#define tangToc ios::sync_with_stdio(0); cin.tie(0)
using namespace std;

int n, cnt = 0;
int a[100001];
stack<int> b;

main()
{
	tangToc;
	cin >> n;
	for(int i = 1; i <= n; i++) cin >> a[i];
	int k = 1;
	for(int i = 1; i <= n; i++)
	{
		while(k < a[i])
		{
			b.push(k);
			k++;
		}
		if(k == a[i])
		{
			k++;
			continue;
		}
		if(k > a[i])
		{
			if(b.top() == a[i])
			{
				b.pop();
				continue;
			}
			else
			{
				cout << "YES\n";
				return 0;
			}
		}
	}
	cout << "NO\n";
}
