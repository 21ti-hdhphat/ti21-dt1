#include<bits/stdc++.h>
using namespace std;

int m, n, cntF = 0, cntC = 0, curF = 0, curC = 0;
char c[1002][1002];

int h[] = {-1, 0, 1, 0}, v[] = {0, 1, 0, -1};
void loang(int x, int y)
{
    c[x][y] = '#';
    for(int i = 0; i < 4; i++)
    {
        char tmp = c[x + h[i]][y + v[i]];
        if(tmp != '#')
        {
            if(tmp == 'c') curC++;
            else if(tmp == 'f') curF++;
            loang(x + h[i], y + v[i]);
        }
    }
}

main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);

    cin >> m >> n;
    for(int i = 1; i <= m; i++)
    {
        string s; cin >> s;
        for(int j = 1; j <= n; j++)
            c[i][j] = s[j-1];
    }

    for(int i = 0; i <= m + 1; i++) c[i][0] = c[i][n+1] = '#';
    for(int j = 0; j <= n + 1; j++) c[0][j] = c[m+1][j] = '#';

    for(int i = 1; i <= m; i++)
    {
        for(int j = 1; j <= n; j++)
        {
            char tmp = c[i][j];
            curF = 0, curC = 0;
            if(tmp != '#')
            {
                if(tmp == 'c') curC++;
                else if(tmp == 'f') curF++;
                c[i][j] = '#';
                loang(i, j);
                (curC > curF) ? cntC += curC : cntF += curF;
            }
        }
    }

    cout << cntF << ' ' << cntC;
}
