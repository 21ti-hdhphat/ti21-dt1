#include<bits/stdc++.h>
#define ll long long
#define tangToc ios::sync_with_stdio(0); cin.tie(0)
using namespace std;

main()
{
	tangToc;
	
	int m, n; cin >> m >> n;
	int a[m+1][n+1];
	
	for(int i = 1; i <= m; i++)
		for(int j = 1; j <= n; j++)
			cin >> a[i][j];
	
	ll s = 0;
	vector<int> h(n+1, 0);
	
	for(int i = 1; i <= m; i++)
	{
		for(int j = 1; j <= n; j++)
			h[j] = (a[i][j] == 9) ? h[j] + 1 : 0;
		
		stack<int> st1, st2;
		vector<int> l(n+1), r(n+1);
		for(int j = 1; j <= n; j++)
		{
			while(st1.size() && h[st1.top()] >= h[j]) st1.pop();
			if(st1.size()) l[j] = st1.top()+1;
			else l[j] = 1;
			st1.push(j);
		}
		
		for(int j = n; j >= 1; j--)
		{
			while(st2.size() && h[st2.top()] >= h[j]) st2.pop();
			if(st2.size()) r[j] = st2.top()-1;
			else r[j] = n;
			st2.push(j);
		}
		
		for(int j = 1; j <= n; j++)
			if(s < (r[j] - l[j] + 1) * 1ll * h[j]) s = (r[j] - l[j] + 1) * 1ll * h[j];
	}
	
	cout << s;
}
